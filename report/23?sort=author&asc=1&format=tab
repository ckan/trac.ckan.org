{{{
cfg = {
    ""mime_types"": [
        (""application/xhtml+xml"", [""html""]),
        (""text/html"", [""html""],
        (""application/rdf+xml"", [""rdf"", ""owl""]),
        (""application/turtle"", [""ttl""]),
        (""text/plain"", [""nt""]),
        (""text/x-graphviz"", [""dot""]),
        ]

for content_type, exts in negotiate(cfg, accept_header):
    if ""html"" in exts: 
        render page normally
    else:
        redirect to semantic.ckan.net
}

}}}"
